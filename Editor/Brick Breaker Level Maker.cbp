<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Brick Breaker Level Editor" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="../Release/bin/Editor_dev" prefix_auto="1" extension_auto="1" />
				<Option working_dir="../Release/bin" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-pg" />
					<Add option="-Og" />
					<Add option="-g" />
					<Add directory="include" />
				</Compiler>
				<Linker>
					<Add option="-pg -lgmon" />
					<Add option="-static" />
					<Add library="raylib" />
					<Add library="opengl32" />
					<Add library="gdi32" />
					<Add library="winmm" />
					<Add directory="lib" />
				</Linker>
			</Target>
			<Target title="Release">
				<Option output="../Release/bin/Editor" prefix_auto="1" extension_auto="1" />
				<Option working_dir="../Release/bin" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O3" />
					<Add directory="include" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add option="-static" />
					<Add library="raylib" />
					<Add library="opengl32" />
					<Add library="gdi32" />
					<Add library="winmm" />
					<Add directory="lib" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-pg" />
			<Add option="-Og" />
			<Add option="-g" />
			<Add option="-fpermissive" />
			<Add directory="../../Raylib Library/raylib/src" />
			<Add directory="../../Raylib Library/raygui/src" />
		</Compiler>
		<Linker>
			<Add option="-pg -lgmon" />
			<Add option="-static-libstdc++" />
			<Add option="-static-libgcc" />
			<Add library="raylib" />
			<Add library="opengl32" />
			<Add library="gdi32" />
			<Add library="winmm" />
			<Add directory="../../Raylib Library/raylib/src" />
		</Linker>
		<Unit filename="editor.cpp" />
		<Unit filename="icon.rc">
			<Option compilerVar="WINDRES" />
		</Unit>
		<Extensions>
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
